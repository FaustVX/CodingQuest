{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "dotnet",
			"task": "build",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [],
			"label": "dotnet: build"
		},
		{
			"type": "process",
			"command": "dotnet",
			"args": ["run", "--configuration", "Release", "--project", "${workspaceFolder}/${workspaceFolderBasename}.App"],
			"problemMatcher": [],
			"label": "run"
		},
	]
}
